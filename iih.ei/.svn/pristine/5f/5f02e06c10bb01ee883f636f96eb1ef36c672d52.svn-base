package iih.ei.een.ws.bean;

import iih.bd.webservice.BaseWsParam;

import java.io.Serializable;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlRootElement;


/**
 * 根据患者刷卡取患者信息bean
 * 
 * @author liubin
 *
 */
@XmlRootElement(name = "data")
@XmlAccessorType(XmlAccessType.FIELD)
public class GetPatInfoBean extends BaseWsParam implements Serializable {

	private static final long serialVersionUID = 1L;
	/**
	 * 患者编码
	 */
	private String code_pat = "";
	/**
	 * 患者姓名
	 */
	private String name_pat = "";
	/**
	 * 性别
	 */
	private String code_dex = "";
	/**
	 * 出生日期
	 */
	private String dt_birth = "";
	/**
	 * 身份证号
	 */
	private String no_id = "";
	/**
	 * 医保卡号
	 */
	private String no_hp = "";
	
	public String getCode_pat() {
		return code_pat;
	}
	public void setCode_pat(String code_pat) {
		this.code_pat = code_pat;
	}
	public String getName_pat() {
		return name_pat;
	}
	public void setName_pat(String name_pat) {
		this.name_pat = name_pat;
	}
	public String getCode_dex() {
		return code_dex;
	}
	public void setCode_dex(String code_dex) {
		this.code_dex = code_dex;
	}
	public String getDt_birth() {
		return dt_birth;
	}
	public void setDt_birth(String dt_birth) {
		this.dt_birth = dt_birth;
	}
	public String getNo_id() {
		return no_id;
	}
	public void setNo_id(String no_id) {
		this.no_id = no_id;
	}
	public String getNo_hp() {
		return no_hp;
	}
	public void setNo_hp(String no_hp) {
		this.no_hp = no_hp;
	}

}
