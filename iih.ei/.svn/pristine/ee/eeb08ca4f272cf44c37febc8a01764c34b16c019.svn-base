package iih.ei.een.ws.bean;

import iih.bd.webservice.BaseWsParam;

import java.io.Serializable;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlRootElement;

/**
 * 
 * 获取科室信息bean
 * 
 * @author liubin
 * 
 * 下午2:40:36
 */
@XmlRootElement(name = "data")
@XmlAccessorType(XmlAccessType.FIELD)
public class GetDepListBean extends BaseWsParam implements Serializable {

	private static final long serialVersionUID = 1L;
	
	/**
	 * 科室id
	 */
	private String id_dep = "";
	/**
	 * 科室编码
	 */
	private String code = "";
	/**
	 * 科室名称
	 */
	private String name = "";
	/**
	 * 科室类型
	 */
	private String sd_deptp = "";
	/**
	 * 简称
	 */
	private String short_name = "";
	/**
	 * 描述
	 */
	private String des = "";
	/**
	 * 是否是末级科室
	 */
	private String fg_leaf = "";
	public String getId_dep() {
		return id_dep;
	}
	public void setId_dep(String id_dep) {
		this.id_dep = id_dep;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSd_deptp() {
		return sd_deptp;
	}
	public void setSd_deptp(String sd_deptp) {
		this.sd_deptp = sd_deptp;
	}
	public String getShort_name() {
		return short_name;
	}
	public void setShort_name(String short_name) {
		this.short_name = short_name;
	}
	public String getDes() {
		return des;
	}
	public void setDes(String des) {
		this.des = des;
	}
	public String getFg_leaf() {
		return fg_leaf;
	}
	public void setFg_leaf(String fg_leaf) {
		this.fg_leaf = fg_leaf;
	}

}
