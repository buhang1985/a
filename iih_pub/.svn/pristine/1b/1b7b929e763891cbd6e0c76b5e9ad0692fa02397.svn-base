package iih.bd.pp.baseprisrv.i;

import iih.bd.pp.baseprisrv.d.PriSrvDO;
import xap.mw.core.data.BizException;

/**
 * 基础价表服务扩展服务接口
 * 
 * @author hao_wu 2019-8-16
 *
 */
public interface IBasePriSrvExtCudService {

	/**
	 * 启用基础价表服务
	 * 
	 * @param priSrvDos
	 * @return
	 * @throws BizException
	 */
	public abstract PriSrvDO[] enable(PriSrvDO[] priSrvDos) throws BizException;

	/**
	 * 停用基础价表服务
	 * 
	 * @param priSrvDos
	 * @return
	 * @throws BizException
	 */
	public abstract PriSrvDO[] disable(PriSrvDO[] priSrvDos) throws BizException;

}
