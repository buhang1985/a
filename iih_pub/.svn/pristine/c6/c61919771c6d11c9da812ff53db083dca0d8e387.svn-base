package iih.mbh.ci.order.d;

import java.util.*;

/**
 * 医嘱信息过滤条件
 * 
 */
public class OrderConditionDTO{
	
	/** 主菜单名称 */
	private String title;
	/** 条件集合 */
	private List<UdidocDTO> udidocs;

	/** getter and setter */
	
	public String getTitle(){
		return this.title;
	}

	public void setTitle(String title){
		this.title = title;
	}
	
	public List<UdidocDTO> getUdidocs(){
		return this.udidocs;
	}

	public void setUdidocs(List<UdidocDTO> udidocs){
		this.udidocs = udidocs;
	}
}