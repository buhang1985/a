package iih.mbh.ci.order.d;


/**
 * 病例信息
 * 
 */
public class CimrInfoDTO{
	
	/** 病历ID */
	private String id_mr;
	/** 就诊ID */
	private String id_ent;
	/** 病历名称 */
	private String name_mr;
	/** 医疗记录录入时间 */
	private String dt_rd;
	/** 医疗记录状态编码 */
	private String sd_su_mr;
	/** 医疗记录状态名称 */
	private String name_su_mr;
	/** 医疗记录类型 */
	private String name_mrtp;
	/** 文件号 */
	private String code_file;
	/** 病历查看地址 */
	private String url;
	/** 医疗记录类型ID */
	private String id_mrtp;

	/** getter and setter */
	
	public String getId_mr(){
		return this.id_mr;
	}

	public void setId_mr(String id_mr){
		this.id_mr = id_mr;
	}
	
	public String getId_ent(){
		return this.id_ent;
	}

	public void setId_ent(String id_ent){
		this.id_ent = id_ent;
	}
	
	public String getName_mr(){
		return this.name_mr;
	}

	public void setName_mr(String name_mr){
		this.name_mr = name_mr;
	}
	
	public String getDt_rd(){
		return this.dt_rd;
	}

	public void setDt_rd(String dt_rd){
		this.dt_rd = dt_rd;
	}
	
	public String getSd_su_mr(){
		return this.sd_su_mr;
	}

	public void setSd_su_mr(String sd_su_mr){
		this.sd_su_mr = sd_su_mr;
	}
	
	public String getName_su_mr(){
		return this.name_su_mr;
	}

	public void setName_su_mr(String name_su_mr){
		this.name_su_mr = name_su_mr;
	}
	
	public String getName_mrtp(){
		return this.name_mrtp;
	}

	public void setName_mrtp(String name_mrtp){
		this.name_mrtp = name_mrtp;
	}
	
	public String getCode_file(){
		return this.code_file;
	}

	public void setCode_file(String code_file){
		this.code_file = code_file;
	}
	
	public String getUrl(){
		return this.url;
	}

	public void setUrl(String url){
		this.url = url;
	}
	
	public String getId_mrtp(){
		return this.id_mrtp;
	}

	public void setId_mrtp(String id_mrtp){
		this.id_mrtp = id_mrtp;
	}
}