package iih.bl.itf.std.bean.input.charge.sub;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;

import iih.bl.itf.std.bean.input.WsParamInfoWithCustom;

/**
 * 门诊补退费新入参记账明细
 * @author shaokx
 * 2019.10.24
 */
@XmlAccessorType(XmlAccessType.FIELD)
public class WsParamCgItm extends  WsParamInfoWithCustom{

	private static final long serialVersionUID = 1L;
	
	/**
	 * 服务编码
	 */
	@XmlElement(name = "chargeCode")
	private String chargeCode;
	
	/**
	 * 物品编码
	 */
	@XmlElement(name = "mmCode")
	private String mmCode;
	
	/**
	 * 数量
	 */
	@XmlElement(name = "quan")
	private String quan;
	
	/**
	 * 单价
	 */
	@XmlElement(name = "price")
	private String price;
	
	/**
	 * 单项折扣比例
	 */
	@XmlElement(name = "priceRatio")
	private String priceRatio;
	
	/**
	 * 实际费用
	 */
	@XmlElement(name = "cgPrice")
	private String cgPrice;
	
	/**
	 * 备注
	 */
	@XmlElement(name = "cgNote")
	private String cgNote;

	public String getChargeCode() {
		return chargeCode;
	}

	public void setChargeCode(String chargeCode) {
		this.chargeCode = chargeCode;
	}

	public String getMmCode() {
		return mmCode;
	}

	public void setMmCode(String mmCode) {
		this.mmCode = mmCode;
	}

	public String getQuan() {
		return quan;
	}

	public void setQuan(String quan) {
		this.quan = quan;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getPriceRatio() {
		return priceRatio;
	}

	public void setPriceRatio(String priceRatio) {
		this.priceRatio = priceRatio;
	}

	public String getCgPrice() {
		return cgPrice;
	}

	public void setCgPrice(String cgPrice) {
		this.cgPrice = cgPrice;
	}

	public String getCgNote() {
		return cgNote;
	}

	public void setCgNote(String cgNote) {
		this.cgNote = cgNote;
	}
	
	
}
