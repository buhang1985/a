package iih.pis.ivx.invoice.d;

public class GetPdfResult {

	//错误码(必填)
	private Integer errcode;
	
	//错误信息(必填)
	private String errmsg;
	
	//pdf 的 url ，两个小时有效期(必填)
	private String pdf_url;
	
	//pdf_url 过期时间， 7200 秒(必填)
	private Integer pdf_url_expire_time;

	public Integer getErrcode() {
		return errcode;
	}

	public String getErrmsg() {
		return errmsg;
	}

	public String getPdf_url() {
		return pdf_url;
	}

	public Integer getPdf_url_expire_time() {
		return pdf_url_expire_time;
	}

	public void setErrcode(Integer errcode) {
		this.errcode = errcode;
	}

	public void setErrmsg(String errmsg) {
		this.errmsg = errmsg;
	}

	public void setPdf_url(String pdf_url) {
		this.pdf_url = pdf_url;
	}

	public void setPdf_url_expire_time(Integer pdf_url_expire_time) {
		this.pdf_url_expire_time = pdf_url_expire_time;
	}
	
}
