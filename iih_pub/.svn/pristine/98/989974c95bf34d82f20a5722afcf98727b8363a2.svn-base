package iih.pis.ivx.invoice.d;

public class QueryInvoceInfoResult {

	//存放除错误码，错误信息外的其他字段(必填)
	private QueryInvoceInfoResult invoicedetail;
	
	//错误码(必填)
	private Integer errcode;
	
	//错误信息(必填)
	private String errmsg;
	
	//发票请求流水号，唯一查询发票的流水号(必填)
	private String fpqqlsh;
	
	//校验码，位于电子发票右上方，开票日期下(必填)
	private String jym;
	
	//校验码(必填)
	private String kprq;
	
	//发票代码(必填)
	private String fpdm;
	
	//发票号码(必填)
	private String fphm;
	
	//发票url(必填)
	private String pdfurl;

	public QueryInvoceInfoResult getInvoicedetail() {
		return invoicedetail;
	}

	public void setInvoicedetail(QueryInvoceInfoResult invoicedetail) {
		this.invoicedetail = invoicedetail;
	}

	public Integer getErrcode() {
		return errcode;
	}

	public String getErrmsg() {
		return errmsg;
	}

	public String getFpqqlsh() {
		return fpqqlsh;
	}

	public String getJym() {
		return jym;
	}

	public String getKprq() {
		return kprq;
	}

	public String getFpdm() {
		return fpdm;
	}

	public String getFphm() {
		return fphm;
	}

	public String getPdfurl() {
		return pdfurl;
	}

	public void setErrcode(Integer errcode) {
		this.errcode = errcode;
	}

	public void setErrmsg(String errmsg) {
		this.errmsg = errmsg;
	}

	public void setFpqqlsh(String fpqqlsh) {
		this.fpqqlsh = fpqqlsh;
	}

	public void setJym(String jym) {
		this.jym = jym;
	}

	public void setKprq(String kprq) {
		this.kprq = kprq;
	}

	public void setFpdm(String fpdm) {
		this.fpdm = fpdm;
	}

	public void setFphm(String fphm) {
		this.fphm = fphm;
	}

	public void setPdfurl(String pdfurl) {
		this.pdfurl = pdfurl;
	}
	
}
