package iih.bl.itf.std.bean.input.charge.sub;

import iih.bl.itf.std.bean.input.WsParamInfoWithCustom;

import java.util.List;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlElementWrapper;

/**
 * 费用清单查询入参--患者信息
 * 
 * @author ly 2018/07/20
 *
 */
@XmlAccessorType(XmlAccessType.FIELD)
public class WsParamBillCostInfo extends WsParamInfoWithCustom {

	private static final long serialVersionUID = 1L;

	// 患者编号
	@XmlElement(name = "patCode")
	private String patCode;

	// 住院次数
	@XmlElement(name = "timesIP")
	private String timesIP;

	// 医嘱编码
	@XmlElement(name = "orderCode")
	private String orderCode;

	// 记账人编码
	@XmlElement(name = "operaCG")
	private String operaCG;

	// 执行时间
	@XmlElement(name = "operaTime")
	private String operaTime;

	// 开立人编码
	@XmlElement(name = "operaOR")
	private String operaOR;

	// 执行人编码
	@XmlElement(name = "operaMP")
	private String operaMP;

	// 执行科室编码
	@XmlElement(name = "mpDepCode")
	private String mpDepCode;

	// 记账科室编码
	@XmlElement(name = "cgDepCode")
	private String cgDepCode;

	// 手术间名称
	@XmlElement(name = "operaRoomNo")
	private String operaRoomNo;

	// 手术名称
	@XmlElement(name = "operaName")
	private String operaName;

	// 高值耗材录入人员编码
	@XmlElement(name = "surgeonCode")
	private String surgeonCode;

	// 费用方向
	@XmlElement(name = "directFlag")
	private String directFlag;

	// 费用标志
	@XmlElement(name = "costFlag")
	private String costFlag;

	// 婴儿标志
	@XmlElement(name = "infantFlag")
	private String infantFlag;

	// 第三方系统补费描述
	@XmlElement(name = "sourceDesc")
	private String sourceDesc;
	// 补费明细
	@XmlElement(name = "chargeItem")
	private List<WsParamBillCostItemInfo> chargeitems;
	public String getPatCode() {
		return patCode;
	}
	public void setPatCode(String patCode) {
		this.patCode = patCode;
	}
	public String getTimesIP() {
		return timesIP;
	}
	public void setTimesIP(String timesIP) {
		this.timesIP = timesIP;
	}
	public String getOrderCode() {
		return orderCode;
	}
	public void setOrderCode(String orderCode) {
		this.orderCode = orderCode;
	}
	public String getOperaCG() {
		return operaCG;
	}
	public void setOperaCG(String operaCG) {
		this.operaCG = operaCG;
	}
	public String getOperaTime() {
		return operaTime;
	}
	public void setOperaTime(String operaTime) {
		this.operaTime = operaTime;
	}
	public String getOperaOR() {
		return operaOR;
	}
	public void setOperaOR(String operaOR) {
		this.operaOR = operaOR;
	}
	public String getOperaMP() {
		return operaMP;
	}
	public void setOperaMP(String operaMP) {
		this.operaMP = operaMP;
	}
	public String getMpDepCode() {
		return mpDepCode;
	}
	public void setMpDepCode(String mpDepCode) {
		this.mpDepCode = mpDepCode;
	}
	public String getCgDepCode() {
		return cgDepCode;
	}
	public void setCgDepCode(String cgDepCode) {
		this.cgDepCode = cgDepCode;
	}
	public String getOperaRoomNo() {
		return operaRoomNo;
	}
	public void setOperaRoomNo(String operaRoomNo) {
		this.operaRoomNo = operaRoomNo;
	}
	public String getOperaName() {
		return operaName;
	}
	public void setOperaName(String operaName) {
		this.operaName = operaName;
	}
	public String getSurgeonCode() {
		return surgeonCode;
	}
	public void setSurgeonCode(String surgeonCode) {
		this.surgeonCode = surgeonCode;
	}
	public String getDirectFlag() {
		return directFlag;
	}
	public void setDirectFlag(String directFlag) {
		this.directFlag = directFlag;
	}
	public String getCostFlag() {
		return costFlag;
	}
	public void setCostFlag(String costFlag) {
		this.costFlag = costFlag;
	}
	public String getInfantFlag() {
		return infantFlag;
	}
	public void setInfantFlag(String infantFlag) {
		this.infantFlag = infantFlag;
	}
	public String getSourceDesc() {
		return sourceDesc;
	}
	public void setSourceDesc(String sourceDesc) {
		this.sourceDesc = sourceDesc;
	}
	public List<WsParamBillCostItemInfo> getChargeitems() {
		return chargeitems;
	}
	public void setChargeitems(List<WsParamBillCostItemInfo> chargeitems) {
		this.chargeitems = chargeitems;
	}
	
	
	

}
