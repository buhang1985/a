package iih.bl.itf.std.bean.output.settle.sub;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;

import iih.bl.itf.std.bean.output.WsResultInfo;

/**
 * 医嘱项目
 * @author hanjq 上午9:20:10
 */
@XmlAccessorType(XmlAccessType.FIELD)
public class WsResultSrv extends WsResultInfo{
	/**
	 * 
	 */
	private static final long serialVersionUID = -8390374299263364357L;

	//项目编码
	@XmlElement(name = "srvCode")
	private String srvCode;
	
	//项目名称
	@XmlElement(name = "srvName")
	private String srvName;
	
	//项目单位
	@XmlElement(name = "unit")
	private String unit;
	
	//规格
	@XmlElement(name = "sepcification")
	private String sepcification;
	
	//项目单价 单位分
	@XmlElement(name = "price")
	private String price;
	
	//项目数量
	@XmlElement(name = "quan")
	private String quan;
	
	//账单项目编码
	@XmlElement(name = "inccacode")
	private String inccacode;
	
	//床位费用 中成药 西成药 治疗费 检查费 化验费 手术费 输氧费 材料费 CT费
	@XmlElement(name = "inccaItm")
	private String inccaItm;
	
	//小计金额
	@XmlElement(name = "amt")
	private String amt;

	public String getSrvCode() {
		return srvCode;
	}

	public void setSrvCode(String srvCode) {
		this.srvCode = srvCode;
	}

	public String getSrvName() {
		return srvName;
	}

	public void setSrvName(String srvName) {
		this.srvName = srvName;
	}

	public String getUnit() {
		return unit;
	}

	public void setUnit(String unit) {
		this.unit = unit;
	}

	public String getSepcification() {
		return sepcification;
	}

	public void setSepcification(String sepcification) {
		this.sepcification = sepcification;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getQuan() {
		return quan;
	}

	public void setQuan(String quan) {
		this.quan = quan;
	}

	public String getInccacode() {
		return inccacode;
	}

	public void setInccacode(String inccacode) {
		this.inccacode = inccacode;
	}

	public String getInccaItm() {
		return inccaItm;
	}

	public void setInccaItm(String inccaItm) {
		this.inccaItm = inccaItm;
	}

	public String getAmt() {
		return amt;
	}

	public void setAmt(String amt) {
		this.amt = amt;
	}
	
	
}
