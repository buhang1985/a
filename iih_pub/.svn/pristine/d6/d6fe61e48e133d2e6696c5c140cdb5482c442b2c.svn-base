package iih.mp.nr.api.help;

import iih.ci.ord.ciorder.d.CiOrderDO;

import java.util.HashMap;

import xap.sys.bdfw.bbd.d.PsnDocDO;
import xap.sys.orgfw.dept.d.DeptDO;

public class AuxiliaryKeyMap {

	private HashMap<String, PsnDocDO> empMapInfo;
	private HashMap<String, DeptDO> deptMapInfo;
	private HashMap<String, CiOrderDO> orderMapInfo;

	/**
	 * 人员信息
	 * 
	 * @return
	 */
	public HashMap<String, PsnDocDO> getEmpMapInfo() {
		return empMapInfo;
	}

	public void setEmpMapInfo(HashMap<String, PsnDocDO> empMapInfo) {
		this.empMapInfo = empMapInfo;
	}

	/**
	 * 部门信息
	 * 
	 * @return
	 */
	public HashMap<String, DeptDO> getDeptMapInfo() {
		return deptMapInfo;
	}

	public void setDeptMapInfo(HashMap<String, DeptDO> deptMapInfo) {
		this.deptMapInfo = deptMapInfo;
	}

	/**
	 * 医嘱信息
	 * 
	 * @return
	 */
	public HashMap<String, CiOrderDO> getOrderMapInfo() {
		return orderMapInfo;
	}

	public void setOrderMapInfo(HashMap<String, CiOrderDO> orderMapInfo) {
		this.orderMapInfo = orderMapInfo;
	}

}
