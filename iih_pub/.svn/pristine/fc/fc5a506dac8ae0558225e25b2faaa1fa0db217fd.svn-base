package iih.ci.log.d;

public enum LogLevel {
	DEBUG("0", "DEBUG"),
	INFO("1","INFO"),
	WARN("2","WARN"),
	ERROR("3","ERROR");
	
	private final String status;
	private final String des;
	
	
	LogLevel(String status, String des) {
		this.status = status;
		this.des = des;
	}
	
	public String getStatus() {
		return status;
	}
	
	public String getDes() {
		return des;
	}
}
