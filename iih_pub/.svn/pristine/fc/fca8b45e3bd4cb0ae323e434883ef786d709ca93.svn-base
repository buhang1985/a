package iih.bl.comm.dto.d;

import xap.mw.core.data.BaseDTO;
import xap.mw.coreitf.d.FBoolean;

/**
 * 费用域校验结果通用DTO
 *
 */
public class BlCheckResultDTO extends BaseDTO {
	private static final long serialVersionUID = 1L;

	/**
	 * 校验结果
	 * @return
	 */
	public FBoolean getResult() {
		return ((FBoolean) getAttrVal("Result"));
	}

	/**
	 * 校验结果
	 * @param result
	 */
	public void setResult(FBoolean result) {
		setAttrVal("Result", result);
	}

	/**
	 * 校验信息
	 * @return
	 */
	public String getMsg() {
		return ((String) getAttrVal("Msg"));
	}

	/**
	 * 校验信息
	 * @param msg
	 */
	public void SetMsg(String msg) {
		setAttrVal("Msg", msg);
	}
}
