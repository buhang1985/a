package iih.bd.srv.mrelement.d;

import xap.sys.appfw.orm.desc.ent.dataobject.DmEnumDesc;

public interface DateTimeFormat {

    @DmEnumDesc(name="None",description="None")
    public static final String TYPE0="None"; //None
    @DmEnumDesc(name="yyyy-MM-dd",description="yyyy-MM-dd")
    public static final String TYPE1="yyyy-MM-dd"; //yyyy-MM-dd
    @DmEnumDesc(name="yyyy-MM-dd HH:mm",description="yyyy-MM-dd HH:mm")
    public static final String TYPE2="yyyy-MM-dd HH:mm"; //yyyy-MM-dd HH:mm
    @DmEnumDesc(name="yyyy/MM/dd",description="yyyy/MM/dd")
    public static final String TYPE3="yyyy/MM/dd"; //yyyy/MM/dd
    @DmEnumDesc(name="yyyy/MM/dd HH:mm",description="yyyy/MM/dd HH:mm")
    public static final String TYPE4="yyyy/MM/dd HH:mm"; //yyyy/MM/dd HH:mm
    @DmEnumDesc(name="yyyy年MM月dd日",description="yyyy年MM月dd日")
    public static final String TYPE5="yyyy年MM月dd日"; //yyyy年MM月dd日
    @DmEnumDesc(name="yyyy年MM月dd日 HH时mm分",description="yyyy年MM月dd日 HH时mm分")
    public static final String TYPE6="yyyy年MM月dd日 HH时mm分"; //yyyy年MM月dd日 HH时mm分
}	
