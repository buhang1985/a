package iih.mbh.nm.nmchkform.d;


/**
 * 单元格下拉列表数据
 * 
 */
public class NmPullDownListDTO{
	
	/** 值id */
	private String id_value;
	/** 值编码 */
	private String sd_value;
	/** 值名称 */
	private String name_value;
	/** 序号 */
	private Integer sortno;

	/** getter and setter */
	
	public String getId_value(){
		return this.id_value;
	}

	public void setId_value(String id_value){
		this.id_value = id_value;
	}
	
	public String getSd_value(){
		return this.sd_value;
	}

	public void setSd_value(String sd_value){
		this.sd_value = sd_value;
	}
	
	public String getName_value(){
		return this.name_value;
	}

	public void setName_value(String name_value){
		this.name_value = name_value;
	}
	
	public Integer getSortno(){
		return this.sortno;
	}

	public void setSortno(Integer sortno){
		this.sortno = sortno;
	}
}