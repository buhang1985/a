package iih.mbh.sys.terminal.d;

import xap.mw.core.data.BaseDO;

/**
 * 终端使用关系
 * 
 */
public class PcUseDTO extends BaseDO{
	
	/** 使用对象 */
	private String id_useobj;
	/** 使用对象名称 */
	private String name_obj;
	/** 使用对象编码 */
	private String code_obj;
	/** 终端序列号 */
	private String pcid;
	/** 终端名称 */
	private String name_pc;
	/** IP地址 */
	private String ip_addr;
	/** Mac地址 */
	private String macaddr;

	/** getter and setter */
	
	public String getId_useobj(){
		return this.id_useobj;
	}

	public void setId_useobj(String id_useobj){
		this.id_useobj = id_useobj;
	}
	
	public String getName_obj(){
		return this.name_obj;
	}

	public void setName_obj(String name_obj){
		this.name_obj = name_obj;
	}
	
	public String getCode_obj(){
		return this.code_obj;
	}

	public void setCode_obj(String code_obj){
		this.code_obj = code_obj;
	}
	
	public String getPcid(){
		return this.pcid;
	}

	public void setPcid(String pcid){
		this.pcid = pcid;
	}
	
	public String getName_pc(){
		return this.name_pc;
	}

	public void setName_pc(String name_pc){
		this.name_pc = name_pc;
	}
	
	public String getIp_addr(){
		return this.ip_addr;
	}

	public void setIp_addr(String ip_addr){
		this.ip_addr = ip_addr;
	}
	
	public String getMacaddr(){
		return this.macaddr;
	}

	public void setMacaddr(String macaddr){
		this.macaddr = macaddr;
	}
}