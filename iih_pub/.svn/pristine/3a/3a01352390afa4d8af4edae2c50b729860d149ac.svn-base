package iih.mbh.nmr.d;

import java.util.*;

/**
 * 护理文书分组
 * 
 */
public class NmrInfoDTO{
	
	/** 文书分组ID */
	private String id_nmr;
	/** 文书分组名称 */
	private String name;
	/** 文书记录集合 */
	private List<NmrRecordDTO> recordlist;
	/** 统一跳转 */
	private Boolean fg_skip;
	/** 文书模板id */
	private String id_nmr_doc;

	/** getter and setter */
	
	public String getId_nmr(){
		return this.id_nmr;
	}

	public void setId_nmr(String id_nmr){
		this.id_nmr = id_nmr;
	}
	
	public String getName(){
		return this.name;
	}

	public void setName(String name){
		this.name = name;
	}
	
	public List<NmrRecordDTO> getRecordlist(){
		return this.recordlist;
	}

	public void setRecordlist(List<NmrRecordDTO> recordlist){
		this.recordlist = recordlist;
	}
	
	public Boolean getFg_skip(){
		return this.fg_skip;
	}

	public void setFg_skip(Boolean fg_skip){
		this.fg_skip = fg_skip;
	}
	
	public String getId_nmr_doc(){
		return this.id_nmr_doc;
	}

	public void setId_nmr_doc(String id_nmr_doc){
		this.id_nmr_doc = id_nmr_doc;
	}
}