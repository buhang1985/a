package iih.mp.mb.i;


import java.util.List;

import org.codehaus.jackson.annotate.JsonProperty;

public class UpPackRecordRequest
{
    @JsonProperty(value = "opera")
    private String opera;
    @JsonProperty(value = "time")
    private String time;
    @JsonProperty(value = "box_code")
    private String box_code;
    @JsonProperty(value = "receiptflag")
    private String receiptflag;
    @JsonProperty(value = "comment")
    private String comment;

    public void setOpera(String opera)
    {
        this.opera = opera;
    }
    public String getOpera()
    {
        return this.opera;
    }
    public void setTime(String time)
    {
        this.time = time;
    }
    public String getTime()
    {
        return this.time;
    }
    public void setBox_code(String box_code)
    {
        this.box_code = box_code;
    }
    public String getBox_code()
    {
        return this.box_code;
    }
    public void setReceiptflag(String receiptflag)
    {
        this.receiptflag = receiptflag;
    }
    public String getReceiptflag()
    {
        return this.receiptflag;
    }
    public void setComment(String comment)
    {
        this.comment = comment;
    }
    public String getComment()
    {
        return this.comment;
    }
}
