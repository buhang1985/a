package iih.bd.srv.mrbasetpl.i;

import iih.bd.srv.mrbasetpl.d.EmrBaseTplDO;
import xap.mw.core.data.BizException;

/**
 * 医疗记录基础模板启用服务
 * 
 * @author hao_wu
 *
 */
public interface IMrBaseTplEnableService {

	/**
	 * 启用医疗记录基础模板
	 * 
	 * @param emrBaseTplDOs
	 *            医疗记录基础模板列表
	 * @return
	 * @throws BizException
	 */
	public abstract EmrBaseTplDO[] Enable(EmrBaseTplDO[] emrBaseTplDOs) throws BizException;

	/**
	 * 停用医疗记录基础模板
	 * 
	 * @param emrBaseTplDOs
	 *            医疗记录基础模板列表
	 * @return
	 * @throws BizException
	 */
	public abstract EmrBaseTplDO[] Disable(EmrBaseTplDO[] emrBaseTplDOs) throws BizException;
}
