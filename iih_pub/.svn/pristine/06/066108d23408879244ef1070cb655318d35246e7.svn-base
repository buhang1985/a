package iih.mbh.sys.login.d;

import java.util.List;

public class MenuItemDTO {

	// 子菜单集合
	private List<MenuItemDTO> childList;
	// 节点描述
	private String des_menuitem;
	// 功能类名
	private String funclass;
	// 节点编码
	private String funcode;
	// 图片
	private String icon;
	// 功能主键
	private String id_fun;
	// 节点名称
	private String name_menuitem;
	// 父级节点
	private String parfuncode;
	// 序号
	private int sno;

	public List<MenuItemDTO> getChildList() {
		return childList;
	}

	public void setChildList(List<MenuItemDTO> childList) {
		this.childList = childList;
	}

	public String getDes_menuitem() {
		return des_menuitem;
	}

	public void setDes_menuitem(String des_menuitem) {
		this.des_menuitem = des_menuitem;
	}

	public String getFunclass() {
		return funclass;
	}

	public void setFunclass(String funclass) {
		this.funclass = funclass;
	}

	public String getFuncode() {
		return funcode;
	}

	public void setFuncode(String funcode) {
		this.funcode = funcode;
	}

	public String getIcon() {
		return icon;
	}

	public void setIcon(String icon) {
		this.icon = icon;
	}

	public String getId_fun() {
		return id_fun;
	}

	public void setId_fun(String id_fun) {
		this.id_fun = id_fun;
	}

	public String getName_menuitem() {
		return name_menuitem;
	}

	public void setName_menuitem(String name_menuitem) {
		this.name_menuitem = name_menuitem;
	}

	public String getParfuncode() {
		return parfuncode;
	}

	public void setParfuncode(String parfuncode) {
		this.parfuncode = parfuncode;
	}

	public int getSno() {
		return sno;
	}

	public void setSno(int sno) {
		this.sno = sno;
	}

}
