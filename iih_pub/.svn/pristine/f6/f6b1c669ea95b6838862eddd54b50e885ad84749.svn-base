package iih.pis.ivx.invoice.d;

public class SetPayMchParam {

	//微信商户号与开票平台关系信息(必填)
	private SetPayMchParam paymch_info;
	
	//微信支付商户号(必填)
	private String mchid;
	
	//为该商户提供开票服务的开票平台 id ，由开票平台提供给商户(必填)
	private String s_pappid;

	public SetPayMchParam getPaymch_info() {
		return paymch_info;
	}

	public String getMchid() {
		return mchid;
	}

	public String getS_pappid() {
		return s_pappid;
	}

	public void setPaymch_info(SetPayMchParam paymch_info) {
		this.paymch_info = paymch_info;
	}

	public void setMchid(String mchid) {
		this.mchid = mchid;
	}

	public void setS_pappid(String s_pappid) {
		this.s_pappid = s_pappid;
	}
	
}
