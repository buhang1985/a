package iih.mbh.nm.nhrflowdata.d;

import iih.mbh.nm.nhroverworkdto.d.NhrOverWorkDTO;
import iih.mbh.nm.nhrschedlvdto.d.NhrSchedLvDTO;
import iih.mbh.nm.nhrschedrpdto.d.NhrSchedRpDTO;

import java.util.List;

public class PagingRtnDataDTO {

	// 唯一标识
	private String id;
	// 是否返回总记录数
	private boolean isrdcountreturned;
	// 加班申请数据
	private List<NhrOverWorkDTO> overworkdata;
	// 总页数
	private int pagecount;
	// 页索引
	private int pageindex;
	// 页大小
	private int pagesize;
	// 总记录数
	private int recordscount;
	// 请假申请数据
	private List<NhrSchedLvDTO> schedlvdata;
	// 替班申请数据
	private List<NhrSchedRpDTO> schedrpdata;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public boolean getIsrdcountreturned() {
		return isrdcountreturned;
	}

	public void setIsrdcountreturned(boolean isrdcountreturned) {
		this.isrdcountreturned = isrdcountreturned;
	}

	public List<NhrOverWorkDTO> getOverworkdata() {
		return overworkdata;
	}

	public void setOverworkdata(List<NhrOverWorkDTO> overworkdata) {
		this.overworkdata = overworkdata;
	}

	public int getPagecount() {
		return pagecount;
	}

	public void setPagecount(int pagecount) {
		this.pagecount = pagecount;
	}

	public int getPageindex() {
		return pageindex;
	}

	public void setPageindex(int pageindex) {
		this.pageindex = pageindex;
	}

	public int getPagesize() {
		return pagesize;
	}

	public void setPagesize(int pagesize) {
		this.pagesize = pagesize;
	}

	public int getRecordscount() {
		return recordscount;
	}

	public void setRecordscount(int recordscount) {
		this.recordscount = recordscount;
	}

	public List<NhrSchedLvDTO> getSchedlvdata() {
		return schedlvdata;
	}

	public void setSchedlvdata(List<NhrSchedLvDTO> schedlvdata) {
		this.schedlvdata = schedlvdata;
	}

	public List<NhrSchedRpDTO> getSchedrpdata() {
		return schedrpdata;
	}

	public void setSchedrpdata(List<NhrSchedRpDTO> schedrpdata) {
		this.schedrpdata = schedrpdata;
	}

}
