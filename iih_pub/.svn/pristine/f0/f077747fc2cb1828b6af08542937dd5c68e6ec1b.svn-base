package iih.ei.std.d.v1.ci.base;

import java.util.ArrayList;
/**
 *  
 * @author zhangwq
 *
 */
public class StringList extends ArrayList<String> {
	public String AsString(){
		StringBuilder strs = new StringBuilder();
		for(String str : this){
			strs.append(str);
		}
		return strs.toString();
	}
}
