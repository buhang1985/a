package iih.bd.res.workstation.i;

import xap.mw.core.data.BizException;

/**
 * 终端注册服务
 * 
 * @author hao_wu
 *
 */
public interface IHostRegService {
	/**
	 * 终端注册
	 * 
	 * @param hostId
	 *            主机id
	 * @param hostName
	 *            主机名称
	 * @param ip_Addr
	 *            ip地址
	 * @param desc
	 *            备注
	 * @param osName
	 *            系统名称
	 * @param memInfo
	 *            内存信息
	 * @param macAddr
	 *            Mac地址
	 * @return
	 * @throws BizException
	 */
	public abstract boolean HostReg(String hostId, String hostName, String ip_Addr, String desc, String osName,
			String memInfo, String macAddr) throws BizException;
}
