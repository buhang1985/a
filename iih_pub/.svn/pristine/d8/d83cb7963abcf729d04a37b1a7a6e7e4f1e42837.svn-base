package iih.bd.mm.materialnames.d.adapter;

import xap.mw.core.data.BaseDO;

/**
 * 基础适配器
 * 
 * @author hao_wu
 *
 */
public class BaseAdapter {

	/**
	 * 设置Do通用属性
	 */
	protected void setDOCommonProp(BaseDO basedo1, BaseDO basedo2) {
		basedo1.setStatus(basedo2.getStatus());
		basedo1.setAttrVal("Sv", basedo2.getAttrVal("Sv"));
	}

}
