package iih.mbh.nm.nmudidoc.d;

public class NmUdiDocDTO {

	// 档案项编码
	private String code;
	// 上级档案项
	private String id_parent;
	// 档案主键
	private String id_udidoc;
	// 档案外键
	private String id_udidoclist;
	// 档案项名称
	private String name;
	// 简称
	private String shortname;

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getId_parent() {
		return id_parent;
	}

	public void setId_parent(String id_parent) {
		this.id_parent = id_parent;
	}

	public String getId_udidoc() {
		return id_udidoc;
	}

	public void setId_udidoc(String id_udidoc) {
		this.id_udidoc = id_udidoc;
	}

	public String getId_udidoclist() {
		return id_udidoclist;
	}

	public void setId_udidoclist(String id_udidoclist) {
		this.id_udidoclist = id_udidoclist;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getShortname() {
		return shortname;
	}

	public void setShortname(String shortname) {
		this.shortname = shortname;
	}

}
