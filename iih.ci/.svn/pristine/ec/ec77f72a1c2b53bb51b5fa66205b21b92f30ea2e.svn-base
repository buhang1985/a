package iih.ci.ord.s.ems.third.base;

import java.util.List;

import iih.ci.ord.ciorder.d.CiorderAggDO;
import iih.ci.ord.ordsrvdose.d.OrdSrvDoseDO;
import iih.ci.ord.ordsrvmm.d.OrdSrvMmDO;
import iih.ci.ord.ordsrvset.d.OrdSrvSetDO;

/**
 * 医嘱包计算结构 ，用来保存过程中使用
 * @author wangqingzhu
 *
 */
public class OrderPakageInfo {
	private Object uiModel;
	private CiorderAggDO orderAggInfo;
	
	private List<OrdSrvMmDO> orderSrvMmList;
	private List<OrdSrvSetDO> orderSrvSetList;
	private List<OrdSrvDoseDO> orderSrvDoseList;
	private List<Object> orderAppSheetList;
	
	// 附加信息
	private List<Object> extInfoList;
	
	
	public CiorderAggDO getOrderAggInfo() {
		return orderAggInfo;
	}

	public void setOrderAggInfo(CiorderAggDO orderAggInfo) {
		this.orderAggInfo = orderAggInfo;
	}

	public List<OrdSrvMmDO> getOrderSrvMmList() {
		return orderSrvMmList;
	}

	public void setOrderSrvMmList(List<OrdSrvMmDO> orderSrvMmList) {
		this.orderSrvMmList = orderSrvMmList;
	}

	public List<Object> getExtInfoList() {
		return extInfoList;
	}

	public void setExtInfoList(List<Object> extInfoList) {
		this.extInfoList = extInfoList;
	}

	public List<OrdSrvSetDO> getOrderSrvSetList() {
		return orderSrvSetList;
	}

	public void setOrderSrvSetList(List<OrdSrvSetDO> orderSrvSetList) {
		this.orderSrvSetList = orderSrvSetList;
	}

	public Object getUiModel() {
		return uiModel;
	}

	public void setUiModel(Object uiModel) {
		this.uiModel = uiModel;
	}

	public List<OrdSrvDoseDO> getOrderSrvDoseList() {
		return orderSrvDoseList;
	}

	public void setOrderSrvDoseList(List<OrdSrvDoseDO> orderSrvDoseList) {
		this.orderSrvDoseList = orderSrvDoseList;
	}

	public List<Object> getOrderAppSheetList() {
		return orderAppSheetList;
	}

	public void setOrderAppSheetList(List<Object> orderAppSheetList) {
		this.orderAppSheetList = orderAppSheetList;
	}


}
