package iih.mp.dg.ipbaseticdrugratio;

/**
 * 基本药物使用率DTO
 * 
 * @author dj.zhang
 *
 */
public class IpBaseTicDrugRatioDTO {

	private String depname;
	private Integer usebasedrugtype;
	private Integer basedruptype;
	private Double configratio;
	private Double sumbaseprice;
	private Double sumdrugprice;
	private Double amountratio;

	public String getDepname() {
		return depname;
	}

	public void setDepname(String depname) {
		this.depname = depname;
	}

	public Integer getUsebasedrugtype() {
		return usebasedrugtype;
	}

	public void setUsebasedrugtype(Integer usebasedrugtype) {
		this.usebasedrugtype = usebasedrugtype;
	}

	public Integer getBasedruptype() {
		return basedruptype;
	}

	public void setBasedruptype(Integer basedruptype) {
		this.basedruptype = basedruptype;
	}

	public Double getConfigratio() {
		return configratio;
	}

	public void setConfigratio(Double configratio) {
		this.configratio = configratio;
	}

	public Double getSumbaseprice() {
		return sumbaseprice;
	}

	public void setSumbaseprice(Double sumbaseprice) {
		this.sumbaseprice = sumbaseprice;
	}

	public Double getSumdrugprice() {
		return sumdrugprice;
	}

	public void setSumdrugprice(Double sumdrugprice) {
		this.sumdrugprice = sumdrugprice;
	}

	public Double getAmountratio() {
		return amountratio;
	}

	public void setAmountratio(Double amountratio) {
		this.amountratio = amountratio;
	}
}
