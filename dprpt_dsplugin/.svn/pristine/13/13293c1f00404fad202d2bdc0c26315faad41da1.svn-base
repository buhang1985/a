package iih.mp.orm.surgplansearch;

public class SurgItemDTO {

	private String id_apop;//手术申请单主键
	private String srv_name;//项目名称
	private String val;//指标的值
	private String items;//将项目名称和值拼接起来
	
	public String getItems() {
		return items;
	}
	public void setItems(String items) {
		this.items = items;
	}
	public String getId_apop() {
		return id_apop;
	}
	public void setId_apop(String id_apop) {
		this.id_apop = id_apop;
	}
	public String getSrv_name() {
		return srv_name;
	}
	public void setSrv_name(String srv_name) {
		this.srv_name = srv_name;
	}
	public String getVal() {
		return val;
	}
	public void setVal(String val) {
		this.val = val;
	}
	
}
