package iih.nm.nhr.dto;

import xap.mw.coreitf.d.FDate;

/**
 * 描述：报表排班公用类
 * @author guo.xiao
 *
 */
public class SchedDTO{
	/**护理人员id*/
	private String id_nur;
	/**人员id*/
	private String id_psndoc;
	/**人员姓名*/
	private String name;
	/**排班日期*/
	private FDate dt_sched_date;
	/**人力班次名称*/
	private String name_hsc_si;
	/**护理班次名称*/
	private String name_nhr_si;
	/**分组人员序号*/
	private int sortno;
	/**护士层级**/
	private String namelvl;

	public String getId_nur() {
		return id_nur;
	}
	public void setId_nur(String id_nur) {
		this.id_nur = id_nur;
	}
	public String getId_psndoc() {
		return id_psndoc;
	}
	public void setId_psndoc(String id_psndoc) {
		this.id_psndoc = id_psndoc;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public FDate getDt_sched_date() {
		return dt_sched_date;
	}
	public void setDt_sched_date(FDate dt_sched_date) {
		this.dt_sched_date = dt_sched_date;
	}
	public String getName_hsc_si() {
		return name_hsc_si;
	}
	public void setName_hsc_si(String name_hsc_si) {
		this.name_hsc_si = name_hsc_si;
	}
	public String getName_nhr_si() {
		return name_nhr_si;
	}
	public void setName_nhr_si(String name_nhr_si) {
		this.name_nhr_si = name_nhr_si;
	}
	public int getSortno() {
		return sortno;
	}
	public void setSortno(int sortno) {
		this.sortno = sortno;
	}
	
	public String getNamelvl() {
		return namelvl;
	}

	public void setNamelvl(String namelvl) {
		this.namelvl = namelvl;
	}
	
}
