package iih.bl.pay.prepaymonth;

import xap.mw.coreitf.d.FDouble;

public class BlPrePayMonthDTO {

	private String prepay_Date;//日期
	private FDouble add_Amt;//本期增加
	private FDouble reduce_Amt;//本期减少	
	private FDouble bala_Amt;//本期余额
	private FDouble bala_AmtSum;//累计余额
	
	public String getPrepay_Date() {
		return prepay_Date;
	}
	public void setPrepay_Date(String prepay_Date) {
		this.prepay_Date = prepay_Date;
	}
	public FDouble getAdd_Amt() {
		return add_Amt;
	}
	public void setAdd_Amt(FDouble add_Amt) {
		this.add_Amt = add_Amt;
	}
	public FDouble getReduce_Amt() {
		return reduce_Amt;
	}
	public void setReduce_Amt(FDouble reduce_Amt) {
		this.reduce_Amt = reduce_Amt;
	}
	public FDouble getBala_Amt() {
		return bala_Amt;
	}
	public void setBala_Amt(FDouble bala_Amt) {
		this.bala_Amt = bala_Amt;
	}
	public FDouble getBala_AmtSum() {
		return bala_AmtSum;
	}
	public void setBala_AmtSum(FDouble bala_AmtSum) {
		this.bala_AmtSum = bala_AmtSum;
	}
}
